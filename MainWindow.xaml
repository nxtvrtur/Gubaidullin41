<Window x:Class="Gubaidullin41size.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Gubaidullin41size"
        mc:Ignorable="d"
        Title="41 размер" Icon="pages/res/icon.ico" FontFamily="Comic Sans MS" MinHeight="200" MinWidth="200" Height="550" Width="800">
    <Grid >
        <Grid.RowDefinitions>
            <RowDefinition Height="100"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="30"></RowDefinition>
        </Grid.RowDefinitions>
        <Frame NavigationUIVisibility="Hidden" ContentRendered="MainFrame_ContentRendered" Grid.Row="1" Name="MainFrame"></Frame>
        <Image Source="pages/res/logo.png" HorizontalAlignment="Left" Margin="20,0,0,0" Height="50" Width="85" VerticalAlignment="Center"></Image>
        <TextBlock VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="40" Text="41 размер"></TextBlock>
        <Button x:Name="BtnBack" HorizontalAlignment="Right" Visibility="Hidden" Grid.Column="1" Grid.ColumnSpan="4" Margin="10" Content="Назад" Click="Button_Click_1"></Button>
    </Grid>
</Window>
